<%= simple_form_for([list, bookmark], url: list_bookmarks_path(list)) do |f| %>
  <div class='row'>
    <div class='col'>
      <%= f.association :movie %>
    </div>
    <div class='col'>
      <%= f.input :comment %>
    </div>
    <div class='col'>
      <%= f.submit %>
    </div>
  </div>
<% end %>
